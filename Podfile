target "MusicFeeling" do
  platform :ios, '6.1'

  pod 'UMengAnalytics'
  pod 'UMengFeedback'

#pod 'iVersion', '~> 1.10.6'

  pod 'UMengMessage'

  pod 'TSMessages'

  pod 'AVOSCloud'
  pod 'AVOSCloudSNS'

  pod 'DDBrick', :path => "../DDBrick"
  pod 'WANetwork', :podspec => 'https://raw.githubusercontent.com/amoblin/WANetwork/master/WANetwork.podspec'
end

target 'MusicBox' do
  platform :ios, '8.0'
  use_frameworks!

  pod 'DDTile', :path => '../DDTile'
end
